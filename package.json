{
  "name": "dynamic-api-client",
  "version": "0.0.0",
  "description": "Dynamic JavaScript Client for APIs that have an OpenAPI Definition. Emphasis on Easy Navigation of API Paths.",
  "main": "dynamic-api-client.js",
  "type": "module",
  "files": [
    "dynamic-api-client.js"
  ],
  "scripts": {
    "format": "npx prettier --arrow-parens=avoid --trailing-comma=none --write *.js",
    "test": "node test.js",
    "test:html": "npx concurrently \"npx snowpack build --polyfill-node\" && sh -c \"cd build && npx http-server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DanielJDufour/dynamic-api-client.git"
  },
  "keywords": [
    "api",
    "client",
    "open"
  ],
  "author": "Daniel J. Dufour",
  "license": "CC0-1.0",
  "bugs": {
    "url": "https://github.com/DanielJDufour/dynamic-api-client/issues"
  },
  "homepage": "https://github.com/DanielJDufour/dynamic-api-client#readme",
  "dependencies": {
    "axios": "^0.21.1"
  },
  "devDependencies": {
    "flug": "^2.1.0"
  }
}
